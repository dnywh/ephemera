    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@netlify/functions": "^1.2.0",
    "airtable": "^0.11.4",
    "dotenv": "^16.0.1",
    "luxon": "^2.4.0",
